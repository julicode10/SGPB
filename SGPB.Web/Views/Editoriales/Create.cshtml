@model SGPB.Web.Data.Entities.Editorial

@{
    ViewData["Title"] = "Nueva Editorial";
}

<header class="bg-primary py-5 mt-lg-5">
    <div class="container px-4 px-lg-5 my-2">
        <div class="text-center text-white">
            <h1 class="display-4 fw-bolder">Nueva Editorial</h1>
        </div>
    </div>
</header>
<section class="py-3">
    <div class="card-footer p-4 pt-0 border-bottom-0 bg-transparent  border-bottom">
    </div>
    <div class="container px-5 my-5 px-5">
        <div class="row gx-5 justify-content-center">
            <div class="col-lg-6">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                    <partial name="_Editorial" />

                    <div class="form-group d-flex flex-row justify-content-center alig-items-center">
                        <input type="submit" value="Crear" class="btn btn-outline-primary mr-2" />
                        <a asp-action="Index" class="btn btn-outline-success">Volver a la lista</a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>
@section Scripts {
    @{
    await Html.RenderPartialAsync("_ValidationScriptsPartial");
}
}

